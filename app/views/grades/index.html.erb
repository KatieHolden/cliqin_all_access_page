<h1>Listing grades</h1>

<table>
  <tr>
    <th>Answers</th>
    <th>Points</th>
    <th>Total points</th>
    <th>Class date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @grades.each do |grade| %>
  <tr>
    <td><%= grade.answers %></td>
    <td><%= grade.points %></td>
    <td><%= grade.total_points %></td>
    <td><%= grade.class_date %></td>
  </tr>
<% end %>
</table>

