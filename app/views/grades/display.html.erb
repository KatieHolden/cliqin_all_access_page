<h3> My Grades: <%= params[:course_name] %> </h3>
<br>

<table class="table table-bordered table-striped">
<tr>
<th>Date</th>
<th>Score</th>
<th>Total Points</th>
</tr>



<% @answers.each do |a| %>
	<tr>
	<td><%= a.class_date.strftime("%b %-d, %Y") %></td>
	<td><%= a.get_student_grade_for_date(params[:student_ID], params[:course_ID], a.class_date) %></td>
	<td><%= a.total_points %></td>
	</tr>
<% end %>
</table>

<%= link_to 'Back to Course Page', student_in_course_path(:id => params[:sicID]) %>
